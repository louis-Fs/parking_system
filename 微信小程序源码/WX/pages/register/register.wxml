<!--pages/register/register.wxml-->
<view class="weui-cells__title">用户注册</view>
  <view class="weui-cells weui-cells_after-title">
    <view class="weui-cell ">
      <view class="weui-cell__hd">
        <view class="weui-label">用户名</view>
      </view>
      <view class="weui-cell__bd">
        <input class="weui-input" placeholder="请输入用户名" bindinput="nameInput"/>
      </view>
    </view>
    <view class="weui-cell ">
      <view class="weui-cell__hd">
        <view class="weui-label">密码</view>
      </view>
      <view class="weui-cell__bd">
        <input class="weui-input" placeholder="请输入密码" password bindinput="passInput"/>
      </view>
    </view>
     <view class="weui-cell ">
      <view class="weui-cell__hd">
        <view class="weui-label">车牌号</view>
      </view>
      <view class="weui-cell__bd">
        <input class="weui-input" placeholder="请输入车牌号"  bindinput="carIdInput"/>
      </view>
    </view>
    
  </view>
   <view class="weui-btn-area">
     <button class="weui-btn yellow-btn" hover-class="yellow-hover-class" bindtap="saveTopTips">注册</button>
     <button class="weui-btn yellow-btn" hover-class="yellow-hover-class" bindtap="tologinTopTips">返回</button>
  </view>
